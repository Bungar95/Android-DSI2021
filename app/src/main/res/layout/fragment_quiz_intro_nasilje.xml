<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.Quiz.Introduction.QuizIntroFragmentNasilje">

    <WebView
        android:id="@+id/webViewKviz1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/webViewKviz1">

        <Button
            android:id="@+id/btnNasiljeOsnovna"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Osnovna škola" />

        <Button
            android:id="@+id/btnNasiljeSrednja"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Srednja škola"/>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>